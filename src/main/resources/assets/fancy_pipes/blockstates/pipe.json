{
  "multipart": [
    {
      "apply": {
        "model": "fancy_pipes:block/pipe_base"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "fancy_pipes:block/pipe_connection"
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "fancy_pipes:block/pipe_connection",
        "x": 180
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "fancy_pipes:block/pipe_connection",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "fancy_pipes:block/pipe_connection",
        "x": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "fancy_pipes:block/pipe_connection",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "fancy_pipes:block/pipe_connection",
        "x": 90,
        "y": 270
      }
    }
  ]
}